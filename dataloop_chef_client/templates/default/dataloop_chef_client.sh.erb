#!/bin/bash

output="$(cat <%= @output_path %>)"

if [[ "$(echo \"$output\")" == *OK* ]]; then
    echo "$output" && exit
elif [[ "$(echo \"$output\")" == *WARNING* ]]; then
    echo "$output" && exit 1
elif [[ "$(echo \"$output\")" == *CRITICAL* ]]; then
    echo "$output" && exit 2
elif [[ "$(echo \"$output\")" == *UNKNOWN* ]]; then
    echo "$output" && exit 3
fi
